int fun(int x, int y) {
  double z;
  z = x * y / 1.523;
  return z;
}
double opa(string z, int x) {
  double y;
  int aopa;
  if (z == "abc") {
    y = x * 39.42;
  } else {
    y = x * 53.234;
  }
  return y;
}
{
  int x, y;
