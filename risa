int risa(int x, int y) {
    return x + y;
}
double dudu(double x, double y, double z) {
    return z + (x + y) * z;
}
string opa(int x, double y) {
    int z;
    z = 3.14;
    if (x * z < y) {
        return "legal";
    }
    return "chato";
}
{
    int x, y;
    if (opa(dudu(x, y, x + y), risa(x, y)) == "legal") {
        print("eba");
    } else {
        print("affers");
    }
}

